{% extends "BeatsBundle::html5.html.twig" %}

{% block metas %}
  {{ parent() }}
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
{% endblock %}

{% block stylesheets %}
  {{ parent() }}

  {% stylesheets output="css/ecma-jquery-bootstrap.css"
  '@BeatsBundle/Resources/public/css/font-awesome.css'
  '@BeatsBundle/Resources/public/css/beats-can/v0.1/ecma-jquery-boostrap.css'%}
  <link rel="stylesheet" type="text/css" href="{{ asset_url }}" />
  {% endstylesheets %}

{% endblock %}

{% block javascripts %}

  {% javascripts output="js/ecma-jquery-bootstrap.js"
    '@BeatsBundle/Resources/public/js/beats-can/v0.1/ecma-jquery-boostrap.js' %}
  <script type="text/javascript" src="{{ asset_url }}"></script>
  {% endjavascripts %}

{% endblock %}
